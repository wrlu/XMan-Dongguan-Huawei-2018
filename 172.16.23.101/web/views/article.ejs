      <div class="Simple" id="Simple" style="position:absolute;">
          <b id="Simple_toggle" title="收起" style="cursor:pointer;">目录[+]</b></p>
        <div id="Simple_content" class="Simple_content"></div>
      </div>

<% include foot %>
